{
  "name": "jooplaan/erase-personal-data",
  "description": "WP-CLI command to import a database, erase personal data, and optionally delete the source SQL file",
  "type": "wp-cli-package",
  "homepage": "https://github.com/jooplaan/erase-personal-data",
  "version": "1.5.4",
  "license": "MIT",
  "authors": [
    {
      "name": "Joop Laan",
      "email": "joop@perky.design",
      "homepage": "https://jooplaan.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "wp-cli/wp-cli": "^2.5"
  },
  "require-dev": {
    "wp-cli/wp-cli-tests": "^3.0.11"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "WP_CLI\\ErasePersonalData\\": "src/"
    },
    "files": [
      "command.php"
    ]
  },
  "extra": {
    "commands": [
      "erase-personal-data"
    ]
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
